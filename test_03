1)
str=input("请输入一个二进制:")
d=0
while str:
    d=d*2+(ord(str[0])-ord('0'))
    str=str[1:]
print("十进制为:",d)

2)
n=eval(input())
x=input().split(',')
y=input().split(',')
sum =0
for i in range(n):
    sum+=int(x[i])*int(y[i])
print("x与y的内积为:",sum)

3)
d={"政治":101,"文学":102,"数学":103,"外语":104,"土语":106}
d["化学"]=105
d["数学"]=203
del d["文学"]
for key in d:
    print("{}:{}".format(d[key],key))
